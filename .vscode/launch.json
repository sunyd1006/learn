{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "logToFile": true,
            "python": "/Users/sunyindong/opt/anaconda3/bin/python",
        },
        {
            "name": "g11_launch_sigle_file",
            "type": "cppdbg",
            "request": "launch",
            "preLaunchTask": "g11_compile_sigle_file",
            "program": "${fileDirname}/${fileBasenameNoExtension}.out",
            "args": [],
            "environment": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": true,
            "externalConsole": false,
            "linux": {
                "MIMode": "gdb"
            },
            "osx": {
                "MIMode": "lldb"
            }
        },
    ]
}